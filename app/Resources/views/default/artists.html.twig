{% extends 'default/list.html.twig' %}

{% block grid %}
<div class="container">
  <div class="row grid header">
    <div class="col-6">
      Nome
    </div>
    <div class="col-4">
      Data nascita/formazione
    </div>  
    <div class="col-2"></div>	
  </div>
  {% for row in data %}
  <div class="row grid">
    <div class="col-6">
      {{ row.name }}
    </div>
    <div class="col-4">
      {{ row.birthDate }}
    </div>
    <div class="col-2" id="react-grid-row-buttons"></div>
  </div>
  {% endfor  %}
</div>

<div id="test-react"></div>
{% endblock %}