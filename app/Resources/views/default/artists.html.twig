{% extends 'default/list.html.twig' %}

{% block grid %}
<div class="container">
  <div class="row grid header">
    <div class="col-8">
      Nome
    </div>
    <div class="col-4">
      Data nascita/formazione
    </div>  	
  </div>
  {% for row in data %}
  <div class="row grid">
    <div class="col-8">
      {{ row.name }}
    </div>
    <div class="col-4">
      {{ row.birthDate }}
    </div>
    <div id="grid-row-buttons">
    	
    </div>
  </div>
  {% endfor  %}
</div>

<div id="test-react"></div>
{% endblock %}